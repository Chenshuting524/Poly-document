{
  "title": "Gitbook Test",
  "theme-default": {
    "showLevel": true
  },
  "plugins": [
    "expandable-chapters",
    "custom-favicon",
    "insert-logo",
    "tbfed-pagefooter",
    "hide-element",
    "anchor-navigation-ex",
    "-lunr",
    "-search",
    "search-pro",
    "github",
    "copy-code-button",
    "heading-anchors",
    "-highlight",
    "prism",
    "prism-themes"

  ],
  "pluginsConfig": {
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy 2021 PolyNetwork",
      "modify_label": "Last modification dateï¼š",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "insert-logo": {
      "url": "/resources/logo.svg",
      "style": "background: none; max-height: 30px; min-height: 30px"
    },
    "favicon": "resources/favicon.ico",
    "hide-element": {
      "elements": [
        ".gitbook-link"
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": true,
      "printLog": false,
      "multipleH1": true,
      "mode": "float",
      "showGoTop": true,
      "float": {
        "floatIcon": "fa fa-anchor",
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    },
    "github": {
      "url": "https://github.com/polynetwork"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-okaidia.css"
      ]
    }
  }
}
