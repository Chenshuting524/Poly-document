{
  "title": "Poly Document",
  "theme-default": {
    "showLevel": true
  },
  "plugins": [
    "emphasize",
    "flexible-alerts",
    "lightbox",
    "alerts",
    "expandable-chapters",
    "custom-favicon",
    "insert-logo",
    "tbfed-pagefooter",
    "hide-element",
    "anchor-navigation-ex",
    "-lunr",
    "-search",
    "search-pro",
    "github",
    "code",
    "heading-anchors",
    "-highlight",
    "prism",
    "prism-themes"

  ],
  "flexible-alerts": {
    "style": "callout",
    "comment": {
      "label": "Comment",
      "icon": "fab fa-comments",
      "className": "info"
    }
  },
  "lightbox": {
    "includeJQuery": false,
    "sameUuid": true,
    "options": {
      "resizeDuration": 500,
      "wrapAround": false
    }
  },
  "pluginsConfig": {
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy 2022 PolyNetwork.",
      "modify_label": "Last modification dateï¼š",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "insert-logo": {
      "url": "/resources/logo.svg",
      "style": "background: none; max-height: 30px; min-height: 30px"
    },
    "favicon": "resources/favicon.ico",
    "hide-element": {
      "elements": [
        ".gitbook-link"
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": true,
      "printLog": false,
      "multipleH1": true,
      "mode": "float",
      "showGoTop": true,
      "float": {
        "floatIcon": "fab fa-anchor",
        "showLevelIcon": false,
        "level1Icon": "fab fa-hand-o-right",
        "level2Icon": "fab fa-hand-o-right",
        "level3Icon": "fab fa-hand-o-right"
      }
    },
    "github": {
      "url": "https://github.com/polynetwork"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-okaidia.css"
      ]
    }
  }
}
